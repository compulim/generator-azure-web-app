{
  "name": "generator-azure-web-app",
  "version": "2.0.0",
  "description": "Minimalist Web App generator: Webpack/Rollup + React + Express, deployable to vanilla Node.js, Azure App Service, and IIS",
  "license": "MIT",
  "engines": {
    "node": ">=6.6.0"
  },
  "author": {
    "name": "William Wong",
    "url": "https://github.com/compulim"
  },
  "bugs": {
    "url": "https://github.com/compulim/generator-azure-web-app/issues"
  },
  "homepage": "https://npmjs.com/package/generator-azure-web-app",
  "main": "app.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/compulim/generator-azure-web-app.git"
  },
  "keywords": [
    "azure",
    "app service",
    "web app",
    "yeoman-generator"
  ],
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "deploy": "gulp deploy",
    "host": "npm start",
    "hostdev": "npm run host:dev",
    "host:dev": "cross-env NODE_ENV=development npm start",
    "hostprod": "npm run host:prod",
    "host:prod": "cross-env NODE_ENV=production npm start -- --content-path dist/website/public/",
    "pack": "gulp pack",
    "rebuild": "gulp rebuild",
    "repack": "gulp repack",
    "start": "node lib/app.js"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "cross-env": "^3.1.4",
    "debug": "^2.2.0",
    "deep-assign": "^2.0.0",
    "express": "^4.14.0",
    "inquirer-npm-name": "^2.0.0",
    "qs": "^6.3.0",
    "serve-static": "^1.11.1",
    "yeoman-generator": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "del": "^2.2.2",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-command": "^0.1.2",
    "gulp-install": "^0.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "mkdirp": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "rollup": "^0.40.2",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-stream": "^1.16.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.2.0",
    "write-file-webpack-plugin": "^3.1.8",
    "xml2js": "^0.4.17"
  }
